{
  "name": "@uzkv/groth16-service",
  "version": "0.1.0",
  "type": "module",
  "description": "Off-chain Groth16 verification service with attestor integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "viem": "^2.7.0",
    "abitype": "^1.0.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "zod": "^3.22.4",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/supertest": "^6.0.2",
    "tsx": "^4.7.0",
    "nodemon": "^3.0.2",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "groth16",
    "zk-proof",
    "verification",
    "arbitrum",
    "stylus"
